Title: Elementor Custom-Code #1838
Type: elementor_snippet
Author: vkptm
Last edited: 2023-03-16 16:06:52
--- The comment is visible only for administrators ---
-->
<script>
const cards = document.querySelectorAll('.elementor-post__card1, .elementor-post__card2, .elementor-post__card3');
const windowHeight = window.innerHeight;

function isElementInViewport(element) {
  const rect = element.getBoundingClientRect();
  return (
    rect.top >= 0 &&
    rect.left >= 0 &&
    rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) &&
    rect.right <= (window.innerWidth || document.documentElement.clientWidth)
  );
}

function animateIfInViewport() {
  cards.forEach(card => {
    if (isElementInViewport(card)) {
      card.classList.add('animated');
    }
  });
}

animateIfInViewport();
window.addEventListener('scroll', animateIfInViewport);
</script>

<!-- End of snippet -->

</body>
</html>
